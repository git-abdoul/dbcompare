		<url>@@ENVXXX.DBURL@@</url>
		<user>@@ENVXXX.DBUSER@@</user>
		<pwd>@@ENVXXX.DBPWD@@</pwd>
		<driver>oracle.jdbc.OracleDriver</driver>
		<prefix-schema>@@ENVXXX.DBPREFIXSCHEMA@@</prefix-schema>
		<active>true</active>
	</database>
	<database>
		<name>@@ENVYYY@@</name>
		<url>@@ENVYYY.DBURL@@</url>
		<user>@@ENVYYY.DBUSER@@</user>
