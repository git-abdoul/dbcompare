	<compare>	
		<query>
			<name>workflow</name>
			<check-duplicates>true</check-duplicates>
			<item target="">
				<select>case when wfw_transition.process_org_id != 0 then legal_entity.short_name else 'ALL' end AS po, event_class, status, use_stp_b, possible_action,resulting_status,same_user_b, product_type, msg_type, log_completed_b, kick_cutoff_b, create_task_b, sd_filter,prefered_b,update_only_b,gen_int_event_b,needs_man_auth_b</select>
				<from>@@prefix@@wfw_transition</from>
				<left-join>@@prefix@@legal_entity ON wfw_transition.PROCESS_ORG_ID = legal_entity.LEGAL_ENTITY_ID</left-join>
				<orderby>event_class, status, possible_action, resulting_status</orderby>
				<identifiers>po,event_class,msg_type,product_type,status,possible_action,resulting_status</identifiers>
			</item>
		</query>
	</compare>
	<compare>	
		<query>
			<name>Workflow rules</name>
			<check-duplicates>false</check-duplicates>
			<item target="">
				<select>case when wfw_transition.process_org_id != 0 then legal_entity.short_name else 'ALL' end AS po,event_class,msg_type,product_type,status,possible_action,resulting_status,rule_name</select>
				<from>@@prefix@@wfw_transition</from>
				<left-join>@@prefix@@bo_workflow_rule ON wfw_transition.WORKFLOW_ID=bo_workflow_rule.ID</left-join>
				<left-join>@@prefix@@legal_entity ON wfw_transition.PROCESS_ORG_ID = legal_entity.LEGAL_ENTITY_ID</left-join>				
				<orderby>po,event_class,msg_type,product_type,status,possible_action,resulting_status,rule_name</orderby>
				<identifiers>po,event_class,msg_type,product_type,status,possible_action,resulting_status</identifiers>
			</item>
		</query>
	</compare>
